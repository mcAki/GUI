<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE validators PUBLIC "-//OpenSymphony Group//XWork Validator 1.0.2//EN" "http://www.opensymphony.com/xwork/xwork-validator-1.0.2.dtd">

<validators>


	<field name="supplyInterface.supplyName">
		<field-validator type="requiredstring">
			<param name="trim">true</param>
			<message>请选择供货商</message>
		</field-validator>
	</field>

	<field name="supplyInterface.goods.goodsId">
		<field-validator type="int">	
			<param name="min">1</param>
			<message>请选择商品</message>
		</field-validator>
	</field>
	
	<field name="supplyInterface.stockPrice">
		<field-validator type="required">
			<message>进货价不能为空</message>
		</field-validator>
		<field-validator type="double">
			<param name="min">0</param>
			<message>请进货价必须大于等0</message>
		</field-validator>
	</field>
	
	<field name="supplyInterface.retailPrice">
		<field-validator type="required">
			<message>进货价不能为空</message>
		</field-validator>
		<field-validator type="double">
			<param name="min">0</param>
			<message>请进货价必须大于等0</message>
		</field-validator>
	</field>
	
	
</validators>