<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	
	<package name="gmmanage" namespace="/gmmanage" extends="struts-core">
		<action name="page" class="com.sys.volunteer.gmmanage.GMManageAction">
			<result name="updateUserState">/gm/userState.jsp</result>
			<result name="broadcast">/gm/broadcast.jsp</result>
			<result name="addItems">/gm/addItems.jsp</result>
			<result name="addItemsByCondition">/gm/addItemsByCondition.jsp</result>
			<result name="addItemsByAll">/gm/addItemsByAll.jsp</result>
			<result name="shutdown">/gm/shutdown.jsp</result>
			<result name="loginGame">/gm/loginGame.jsp</result>
			<result name="replyMail">/gm/replyMail.jsp</result>
			<result name="recharge">/gm/recharge.jsp</result>
			<result name="announce">/gm/announce.jsp</result>
		</action>
		
		<action name="doUpdateUserState" class="com.sys.volunteer.gmmanage.GMManageAction" method="doUpdateUserState">
			<interceptor-ref name="token" />
           	<interceptor-ref name="defaultStack" />
			<result name="invalid.token">/common/error2.jsp</result>
			<result name="input">/gm/userState.jsp</result>
		</action>
		
		<action name="doBroadcast" class="com.sys.volunteer.gmmanage.GMManageAction" method="doBroadcast">
			<interceptor-ref name="token" />
           	<interceptor-ref name="defaultStack" />
			<result name="invalid.token">/common/error2.jsp</result>
			<result name="input">/gm/broadcast.jsp</result>
		</action>
		
		<action name="doAddItems" class="com.sys.volunteer.gmmanage.GMManageAction" method="doAddItems">
			<interceptor-ref name="token" />
           	<interceptor-ref name="defaultStack" />
			<result name="invalid.token">/common/error2.jsp</result>
			<result name="input">/gm/addItems.jsp</result>
		</action>
		
		<action name="doAddItemsByCondition" class="com.sys.volunteer.gmmanage.GMManageAction" method="doAddItemsByCondition">
			<interceptor-ref name="token" />
           	<interceptor-ref name="defaultStack" />
			<result name="invalid.token">/common/error2.jsp</result>
			<result name="input">/gm/addItemsByCondition.jsp</result>
		</action>
		
		<action name="doAddItemsByAll" class="com.sys.volunteer.gmmanage.GMManageAction" method="doAddItemsByAll">
			<interceptor-ref name="token" />
           	<interceptor-ref name="defaultStack" />
			<result name="invalid.token">/common/error2.jsp</result>
			<result name="input">/gm/addItemsByAll.jsp</result>
		</action>
		
		<action name="doShutdown" class="com.sys.volunteer.gmmanage.GMManageAction" method="doShutdown">
			<interceptor-ref name="token" />
           	<interceptor-ref name="defaultStack" />
			<result name="invalid.token">/common/error2.jsp</result>
			<result name="input">/gm/shutdown.jsp</result>
		</action>
		
		<action name="doReplyMail" class="com.sys.volunteer.gmmanage.GMManageAction" method="doReplyMail">
			<interceptor-ref name="token" />
           	<interceptor-ref name="defaultStack" />
			<result name="invalid.token">/common/error2.jsp</result>
			<result name="input">/gm/replyMail.jsp</result>
		</action>
		
		<action name="doRecharge" class="com.sys.volunteer.gmmanage.GMManageAction" method="doRecharge">
			<interceptor-ref name="token" />
           	<interceptor-ref name="defaultStack" />
			<result name="invalid.token">/common/error2.jsp</result>
			<result name="input">/gm/recharge.jsp</result>
		</action>
		
		<action name="doAnnounce" class="com.sys.volunteer.gmmanage.GMManageAction" method="doAnnounce">
			<interceptor-ref name="token" />
           	<interceptor-ref name="defaultStack" />
			<result name="invalid.token">/common/error2.jsp</result>
			<result name="input">/gm/announce.jsp</result>
		</action>
		
		<action name="doLoginGame" class="com.sys.volunteer.gmmanage.GMManageAction" method="doLoginGame">
			<result name="success" type="redirect">
				${returnUrl}
			</result>
			
		</action>
		
		<action name="list" class="com.sys.volunteer.gmmanage.ListGMManagerAction">
			<result name="listUserInfo">/gm/listUserInfo.jsp</result>
			<result name="listBroadcastVo">/gm/listBroadcastVo.jsp</result>
			<result name="listItemLogVo">/gm/listItemLogVo.jsp</result>
			<result name="listOnlinePlayer">/gm/listOnlinePlayer.jsp</result>
			<result name="listUserChargeVo">/gm/listUserChargeVo.jsp</result>
			<result name="listUserConsumeVo">/gm/listUserConsumeVo.jsp</result>
			<result name="listPlayerCountByLevel">/gm/listPlayerCountByLevel.jsp</result>
			<result name="listLogintimeInfo">/gm/listLogintimeInfo.jsp</result>
			<result name="listUserLevel">/gm/listUserLevel.jsp</result>
			<result name="listFeedback">/gm/listFeedback.jsp</result>
			<result name="calLoginByEveryday">/gm/calLoginByEveryday.jsp</result>
			<result name="calLivenessPlayer">/gm/calLivenessPlayer.jsp</result>
			<result name="calUserChargeForPlayer">/gm/calUserChargeForPlayer.jsp</result>
			<result name="calOnlinePlayerCount">/gm/calOnlinePlayerCount.jsp</result>
			<result name="calPlayerChurnRate">/gm/calPlayerChurnRate.jsp</result>
			<result name="calPlayerStay">/gm/calPlayerStay.jsp</result>
			<result name="calPlayerStay7Days">/gm/calPlayerStay7Days.jsp</result>
			<result name="calRegister">/gm/calRegister.jsp</result>
			<result name="calPlayerChurnCountByLevel">/gm/calPlayerChurnCountByLevel.jsp</result>
			<result name="calFirstChargeAvg">/gm/calFirstChargeAvg.jsp</result>
			<result name="jfc">/gm/jfc.jsp</result>
		</action>
	</package>
</struts>