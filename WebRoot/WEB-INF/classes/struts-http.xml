<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<package name="http" namespace="/http" extends="struts-core">
		
		<action name="getHttpPage" class="com.sys.volunteer.http.POSAction" method="getHttpPage">
			<result name="show">/http/showHttpPage.jsp</result>
		</action>
		
		<action name="pos" class="com.sys.volunteer.http.POSAction" >
			<result name="page">/http/httpPage.jsp</result>
			<result name="show">/http/showHttpPage.jsp</result>
		</action>
	</package>	
	
</struts>