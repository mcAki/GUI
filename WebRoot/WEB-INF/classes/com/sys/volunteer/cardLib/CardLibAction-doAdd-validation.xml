<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE validators PUBLIC "-//OpenSymphony Group//XWork Validator 1.0.2//EN" "http://www.opensymphony.com/xwork/xwork-validator-1.0.2.dtd">

<validators>

	<field name="cardLib.supply.id">
		<field-validator type="required">
			<param name="trim">true</param>
			<message>供货商不能为空</message>
		</field-validator>
	</field>

	<field name="content">
		<field-validator type="required">
			<param name="trim">true</param>
			<message>卡密内容不能为空</message>
		</field-validator>
	</field>

	<field name="cardLib.expireTime">
		<field-validator type="required">
			<param name="trim">true</param>
			<message>有效期不能为空</message>
		</field-validator>
	</field>
</validators>